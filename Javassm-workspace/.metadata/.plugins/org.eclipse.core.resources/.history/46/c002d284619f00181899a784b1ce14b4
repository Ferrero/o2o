/**
* @author xf
* @version 2018年8月10日 下午3:54:28
*/
public interface ProductCategoryDao {
	/**
	 * 通过shopId 查询店铺商品类别
	 * @param long shopId
	 * @return List<ProductCategory>
	 */
	List<ProductCategory> queryProductCategoryList(long shopId);
}
